    "@vueuse/integrations": "^10.11.0",
    "@vueuse/router": "^10.11.0",
    "fuse.js": "^7.0.0",
    "mande": "^2.0.9",
    "pinia": "^2.1.7",
    "vue": "^3.4.33",
    "vue-router": "^4.4.0",
    "water.css": "^2.1.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^20.14.11",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "json-server": "0.17.4",
    "npm-run-all2": "^6.2.2",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.6",
    "typescript": "~5.5.3",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.3.4",
    "vue-tsc": "^2.0.26"
