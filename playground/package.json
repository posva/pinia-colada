{
  "name": "playground",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run --stream '/^dev:/'",
    "dev:client": "vite",
    "dev:db": "json-server --port 7777 db.json",
